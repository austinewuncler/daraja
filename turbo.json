{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "test": {
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "test/**/*.ts",
        "test/**/*.tsx"
      ],
      "outputs": []
    },
    "build": { "dependsOn": ["^build"], "outputs": ["dist/**"] }
  }
}
